{
	"name": "ol-coords",
	"version": "1.0.1",
	"description": "Fly to coordinates Control for OpenLayers",
	"main": "dist/ol-coords.js",
	"targets": {
		"main": {
			"source": "src/ol-coords.js",
			"distDir": "./dist",
			"outputFormat": "esmodule",
			"isLibrary": true
		},
		"cdn": {
			"source": "src/ol-coords-cdn.js",
			"distDir": "./dist/cdn",
			"outputFormat": "esmodule",
			"isLibrary": true
		}
	},
	"type": "module",
	"browserslist": "> 0.5%, last 2 versions, not dead",
	"scripts": {
		"start": "parcel",
		"build": "rm -r ./dist & parcel build",
		"test": "echo \"Error: no test specified\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bugraaydin1/ol-coords.git"
	},
	"keywords": [
		"openlayers",
		"ol",
		"control",
		"coordinates",
		"coords",
		"goto",
		"fly"
	],
	"author": "Ahmet Buğra Aydın <bugraaydin1@hotmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/bugraaydin1/ol-coords/issues"
	},
	"homepage": "https://github.com/bugraaydin1/ol-coords#readme",
	"dependencies": {
		"@swc/helpers": "^0.2.11"
	},
	"devDependencies": {
		"jest": "^27.3.1",
		"parcel": "^2.0.0"
	},
	"optionalDependencies": {
		"ol": ">6.0.0"
	}
}
